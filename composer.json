{
    "name": "tecnodesignc/page-module",
    "type": "encore-module",
    "description": "Page module for EncoreCMS 2.1. Page management.",
    "keywords": [
        "EncoreCMS",
        "pages",
        "Builder",
        "Website"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Rativa",
            "email": "info@tecnodesign.com.co",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@tecnodesign.com.co",
        "issues": "https://github.com/tecnodesignc/encorecms/issues",
        "source": "https://github.com/tecnodesignc/page-module"
    },
    "require": {
        "php": ">=8.0",
        "composer/installers": "^2.1",
        "tecnodesignc/tag-module": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "3.8.*"
    },
    "autoload": {
        "psr-4": {
            "Modules\\Page\\": ".",
            "Modules\\": "modules/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
