{
    "name": "tecnodesignc/page-module",
    "type": "encore-module",
    "description": "Page module for EncoreCMS 2. Page management.",
    "keywords": [
        "encorecms",
        "pages"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Rativa",
            "email": "marcos21.009@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@tecnodesign.com.co",
        "issues": "https://github.com/tecnodesignc/Page/issues",
        "source": "https://github.com/tecnodesignc/Page"
    },
    "require": {
        "php": ">=8.0",
        "composer/installers": "^2.1",
        "tecnodesignc/tag-module": "^2.0"
